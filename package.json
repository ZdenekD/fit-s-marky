{
  "author": "zdenekd",
  "name": "fit-s-marky",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "homepage": "git@github.com:ZdenekD/fit-s-marky.git#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:ZdenekD/fit-s-marky.git"
  },
  "scripts": {
    "clean": "rm -rf ./public/*",
    "reinstall": "rm -rf 'yarn.lock' 'node_modules' && yarn",
    "start": "NODE_ENV=development webpack-dev-server --mode development --color --compress --open",
    "build": "yarn clean && NODE_ENV=production webpack --mode production && yarn sitemap",
    "sitemap": "node ./node_scripts/sitemap.js",
    "test:snyk": "snyk monitor",
    "test:webhint": "hint http://fit-s-marky.src",
    "test:packages": "yarn outdated"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --cache --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --cache --fix --color --formatter verbose",
      "git add"
    ],
    "*.{json,md,yml,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.7",
    "@hint/configuration-web-recommended": "^8.0.2",
    "@hot-loader/react-dom": "^16.11.0",
    "@loadable/babel-plugin": "^5.11.0",
    "babel-eslint": "8",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.1",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "favicons-webpack-plugin": "^3.0.0",
    "file-loader": "^5.0.2",
    "hint": "^6.0.3",
    "html-replace-webpack-plugin": "^2.5.6",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "husky": "^4.0.10",
    "imagemin-webpack-plugin": "^2.4.2",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
