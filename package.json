  "scripts": {
    "clean": "rm -rf ./dist/*",
    "reinstall": "rm -rf 'yarn.lock' 'package-lock.json' 'node_modules' && yarn",
    "start": "NODE_ENV=development webpack-dev-server --mode development --color --compress --open",
    "format:scripts": "eslint --cache --ext .js,.jsx --fix ./",
    "format:styles": "stylelint \"**/*.css\" --fix --color --formatter verbose",
    "format:prettier": "prettier --write '**/*.{json,md,yml,html}'",
    "format": "yarn format:scripts && yarn format:styles && yarn format:prettier",
    "build": "yarn clean && NODE_ENV=production webpack --mode production",
    "test:snyk": "snyk monitor",
    "test:webhint": "hint http://fit-s-marky.src"
  },
